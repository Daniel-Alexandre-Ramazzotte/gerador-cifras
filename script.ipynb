    "def transpor_nota(nota, shift):\n",
    "    notas_para_numeros = {\n",
    "        'A': 0, 'A#': 1, 'Bb': 1, 'B': 2, 'C': 3, 'C#': 4, 'Db': 4, 'D': 5,\n",
    "        'D#': 6, 'Eb': 6, 'E': 7, 'F': 8, 'F#': 9, 'Gb': 9, 'G': 10, 'G#': 11, 'Ab': 11\n",
    "    }\n",
    "    numeros_para_notas = {v: k for k, v in notas_para_numeros.items()}\n",
    "    \n",
    "    # Extrai a parte da nota base (primeiros caracteres)\n",
    "    if len(nota) >= 2 and nota[1] in ('#', 'b'):\n",
    "        nota_base = nota[:2]  # Caso seja sustenido ou bemol (ex: A#, Bb)\n",
    "        resto = nota[2:]\n",
    "    else:\n",
    "        nota_base = nota[0]  # Caso seja uma nota simples (ex: A, B, C)\n",
    "        resto = nota[1:]\n",
    "    \n",
    "    # Aplica a transposição apenas na nota base\n",
    "    if nota_base in notas_para_numeros:\n",
    "        novo_valor = (notas_para_numeros[nota_base] + shift) % 12\n",
    "        nova_nota_base = numeros_para_notas[novo_valor]\n",
    "        return nova_nota_base + resto  # Retorna a nova nota base + o restante da cifra\n",
    "    \n",
    "    return nota  # Retorna a nota original se não for possível transpor\n",
    "\n",
    "def extrair_cifra(url, shift=None):\n",
    "    \"\"\"\n",
    "    Extrai a cifra de uma música a partir de uma URL do Cifra Club.\n",
    "\n",
    "    :param url: URL da página da cifra.\n",
    "    :param shift: Quantidade de semitons para transpor a cifra. Se None, não transpõe.\n",
    "    :return: String com a cifra extraída.\n",
    "    \"\"\"\n",
    "    try:\n",
    "        # Faz a requisição HTTP para abrir o link\n",
    "        print(f\"Acessando o link: {url}\")\n",
    "        site = requests.get(url, headers=headers)\n",
    "        site.raise_for_status()  # Verifica se a requisição foi bem-sucedida\n",
    "\n",
    "        # Parseia o conteúdo HTML\n",
    "        soup = BeautifulSoup(site.content, 'html.parser')\n",
    "\n",
    "        # Remove as tags <span> que contêm a classe 'tablatura'\n",
    "        for tablatura in soup.find_all('span', class_='tablatura'):\n",
    "            tablatura.decompose()\n",
    "            \n",
    "        # Captura o tom da música\n",
    "        tom_tag = soup.find('a', class_='js-modal-trigger', title=\"alterar o tom da cifra\")\n",
    "        tom = tom_tag.text.strip() if tom_tag else None\n",
    "\n",
    "        # Captura o capotraste\n",
    "        capo_tag = soup.find('a', class_='js-modal-trigger', title=\"alterar capotraste\")\n",
    "        if capo_tag:\n",
    "            capo_texto = capo_tag.text.strip()\n",
    "            # Extrai o número da casa do capotraste\n",
    "            capo = int(capo_texto.split('ª')[0]) if 'ª' in capo_texto else 0\n",
    "        else:\n",
    "            capo = 0  # Se não houver capotraste, define como 0\n",
    "\n",
    "        # Encontra todas as tags <pre> que contêm a cifra\n",
    "        pre_tags = soup.find_all('pre')\n",
    "        \n",
    "        # Se um shift for fornecido, transpõe as notas dentro das tags <b>\n",
    "        if not pd.isna(shift):\n",
    "            if capo != 0:\n",
    "                capo = capo + shift\n",
    "            tom = transpor_nota(tom,shift)\n",
    "            for pre in pre_tags:\n",
    "                for b_tag in pre.find_all('b'):\n",
    "                    nota = b_tag.get_text()\n",
    "                    nova_nota = transpor_nota(nota, shift)\n",
    "                    b_tag.string.replace_with(nova_nota)\n",
    "                \n",
    "        # Extrai o texto, removendo linhas em branco extras\n",
    "        cifras = []\n",
    "        for pre in pre_tags:\n",
    "            linhas = pre.get_text().splitlines()\n",
    "            linhas_filtradas = [linha for linha in linhas if linha.strip()]\n",
    "            cifras.append(\"\\n\".join(linhas_filtradas))\n",
    "\n",
    "        \n",
    "\n",
    "        return {\n",
    "            'cifra': \"\\n\".join(cifras),\n",
    "            'tom': tom,\n",
    "            'capo': capo\n",
    "        } # Junta todas as cifras em uma única string\n",
    "    except Exception as e:\n",
    "        print(f\"Erro ao extrair a cifra: {e}\")\n",
    "        return None\n",
    "\n",
    "\n",
    "# Gera um PDF com as cifras\n",
    "def criar_pdf(cifras_info, output_file=\"cifra.pdf\", fonte=\"Courier\", tamanho_fonte=12):\n",
    "    \"\"\"\n",
    "    Cria um PDF com as cifras e títulos fornecidos, incluindo um cabeçalho com o tom e o capotraste.\n",
    "\n",
    "    :param cifras_info: Lista de tuplas contendo o título da música, a cifra, o tom e o capotraste.\n",
    "    :param output_file: Nome do arquivo PDF de saída.\n",
    "    :param fonte: Fonte a ser usada no PDF.\n",
    "    :param tamanho_fonte: Tamanho da fonte.\n",
    "    \"\"\"\n",
    "    pdf = canvas.Canvas(output_file, pagesize=A4)\n",
    "    largura_pagina, altura_pagina = A4\n",
    "    margem_esquerda = 50\n",
    "    margem_direita = largura_pagina / 2 + 20\n",
    "    centro_x = largura_pagina / 2\n",
    "    margem_superior = altura_pagina - 50\n",
    "    margem_inferior = 50\n",
    "    espacamento_linhas = tamanho_fonte + 2\n",
    "    espacamento_titulos = 30\n",
    "\n",
    "    primeira_pagina = True\n",
    "\n",
    "    for titulo, cifra, tom, capo in cifras_info:\n",
    "        if not primeira_pagina:\n",
    "            pdf.showPage()  # Cria uma nova página\n",
    "        primeira_pagina = False\n",
    "\n",
    "        # Adiciona o cabeçalho com o tom e o capotraste\n",
    "        pdf.setFont(\"Helvetica-Bold\", 12)\n",
    "        cabecalho = f\"Tom: {tom}\"\n",
    "        if capo != 0:\n",
    "            cabecalho += f\" | Capo: {capo}\"\n",
    "        pdf.drawString(margem_esquerda, margem_superior - 20, cabecalho)\n",
    "\n",
    "        # Adiciona o título da música\n",
    "        pdf.setFont(\"Helvetica-Bold\", 16)\n",
    "        pdf.drawCentredString(centro_x, margem_superior - 40, titulo)\n",
    "\n",
    "        # Adiciona a cifra\n",
    "        pdf.setFont(fonte, tamanho_fonte)\n",
    "        y_coluna_esquerda = margem_superior - 70  # Ajuste para o cabeçalho\n",
    "        y_coluna_direita = y_coluna_esquerda\n",
    "        x_coluna_esquerda = margem_esquerda\n",
    "        x_coluna_direita = margem_direita\n",
    "        coluna_atual = 1\n",
    "\n",
    "        for line in cifra.split(\"\\n\"):\n",
    "            if coluna_atual == 1:\n",
    "                if y_coluna_esquerda < margem_inferior:\n",
    "                    coluna_atual = 2\n",
    "                else:\n",
    "                    pdf.drawString(x_coluna_esquerda, y_coluna_esquerda, line)\n",
    "                    y_coluna_esquerda -= espacamento_linhas\n",
    "            if coluna_atual == 2:\n",
    "                if y_coluna_direita < margem_inferior:\n",
    "                    break\n",
    "                pdf.drawString(x_coluna_direita, y_coluna_direita, line)\n",
    "                y_coluna_direita -= espacamento_linhas\n",
    "\n",
    "    pdf.save()\n",
    "    print(f\"PDF salvo como {output_file}\")\n",
    "\n",
    "# Função para criar a coluna de links\n",
    "def criar_coluna_links(df):\n",
    "    try:\n",
    "        def gerar_link(row):\n",
    "            artista = row[1].lower().replace(\" \", \"-\").replace(\"'\", \"\").replace(\"ã\", \"a\")\\\n",
    "                .replace(\"á\", \"a\").replace(\"â\", \"a\").replace(\"é\", \"e\").replace(\"ê\", \"e\")\\\n",
    "                .replace(\"í\", \"i\").replace(\"ó\", \"o\").replace(\"ô\", \"o\").replace(\"ú\", \"u\")\\\n",
    "                .replace(\"ç\", \"c\")\n",
    "            musica = row[0].lower().replace(\" \", \"-\").replace(\"'\", \"\").replace(\"ã\", \"a\")\\\n",
    "                .replace(\"á\", \"a\").replace(\"â\", \"a\").replace(\"é\", \"e\").replace(\"ê\", \"e\")\\\n",
    "                .replace(\"í\", \"i\").replace(\"ó\", \"o\").replace(\"ô\", \"o\").replace(\"ú\", \"u\")\\\n",
    "                .replace(\"ç\", \"c\")\n",
    "\n",
    "            link = f\"https://www.cifraclub.com.br/{artista}/{musica}/\"\n",
    "\n",
    "            if row['Simplificada'] != 0:\n",
    "                link += \"simplificada.html\"\n",
    "            return link\n",
    "\n",
    "        df['Link'] = df.apply(gerar_link, axis=1)\n",
    "        print(\"Coluna de links criada com sucesso!\")\n",
    "        return df\n",
    "\n",
    "    except Exception as e:\n",
    "        print(f\"Erro ao criar coluna de links: {e}\")\n",
    "        return None"
   ]
